<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Github remote -->
  <remote  name="github"
           fetch="https://github.com/" />

  <!-- Gitlab remote -->
  <remote  name="gitlab"
           fetch="https://gitlab.com/" />

  <!-- WestCoastRomS remote -->
  <remote  name="WestCoastRomS"
           revision="wcr-9.0"
           fetch="https://github.com/WestCoastRomS/" />

  <!-- WestCoastRomS REPOS -->

  <!-- General -->
  <project path="system/settings/apps" name="AnimeWallpapers" remote="WestCoastRomS" />
  <project path="device/google/Dank_sarnito" name="Dank_sarnito" remote="WestCoastRomS" />
  <project path="device/vendor/google" name="vendor_google" remote='WestCoastRomS" />

  <!-- Vendor -->
  <project path="vendor/du" name="android_vendor_du" remote="WestCoastRomS" >
    <linkfile src="scripts/merge-aosp.sh" dest="merge-aosp.sh" />
  </project>
  <project path="vendor/extras" name="android_vendor_extras" remote="WestCoastRomS" />
  <project path="vendor/themes" name="android_vendor_themes" remote="WestCoastRomS" />

</manifest>

    

